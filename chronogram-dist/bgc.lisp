((LANGUAGE . "bgc")
 (CALENDARS
  (GREGORIAN
   (MONTHS
    (FORMAT
     (WIDE
      (DEFAULT (12 . "दिसंबर") (11 . "नवम्बर") (10 . "अक्टूबर") (9 . "सितम्बर")
       (8 . "अगस्त") (7 . "जुलाई") (6 . "जून") (5 . "मई") (4 . "अप्रैल")
       (3 . "मार्च") (2 . "फरवरी") (1 . "जनवरी")))))
   (DAYS
    (FORMAT
     (WIDE
      (DEFAULT (SAT . "शनिवार") (FRI . "शुक्रवार") (THU . "बृहस्पतवार")
       (WED . "बुधवार") (TUE . "मंगलवार") (MON . "सोमवार") (SUN . "ऐतवार")))))
   (DAY-PERIODS (FORMAT (WIDE (DEFAULT (PM . "सांझ") (AM . "तडके")))))
   (TIME-FORMATS (SHORT (PATTERN . "h:mm a")) (MEDIUM (PATTERN . "h:mm:ss a"))
    (LONG (PATTERN . "h:mm:ss a z")) (FULL (PATTERN . "h:mm:ss a zzzz"))))))
