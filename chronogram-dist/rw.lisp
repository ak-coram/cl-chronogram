      (DEFAULT (SAT . "gnd.") (FRI . "gnu.") (THU . "kan.") (WED . "gtu.")
       (TUE . "kab.") (MON . "mbe.") (SUN . "cyu.")))))
   (QUARTERS
    (FORMAT
     (WIDE
      (DEFAULT (4 . "igihembwe cya kane") (3 . "igihembwe cya gatatu")
       (2 . "igihembwe cya kabiri") (1 . "igihembwe cya mbere")))
