((LANGUAGE . "lmo")
 (CALENDARS
  (GREGORIAN
   (MONTHS
    (FORMAT
     (WIDE
      (DEFAULT (12 . "dicember") (11 . "november") (10 . "otover")
       (9 . "setember") (8 . "avost") (7 . "luj") (6 . "sgiugn") (5 . "masg")
       (4 . "avril") (3 . "marz") (2 . "fevrer") (1 . "sginer")))))
   (DAYS
    (FORMAT
     (WIDE
      (DEFAULT (SAT . "sabet") (FRI . "venerdì") (THU . "sgiovedì")
       (WED . "mercoldì") (TUE . "mardì") (MON . "lundì")
       (SUN . "domenega")))))
   (DAY-PERIODS
    (FORMAT (WIDE (DEFAULT (PM . "de sira") (AM . "del matin"))))))))
